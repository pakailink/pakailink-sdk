{
    "name": "pakailink/pakailink-sdk",
    "description": "PakaiLink Payment Gateway SDK for Laravel - SNAP compliant payment integration",
    "keywords": [
        "pakailink",
        "payment",
        "gateway",
        "snap",
        "laravel",
        "indonesia",
        "virtual-account",
        "qris",
        "e-wallet"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "PGPay Team",
            "email": "dev@pgpay.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/cache": "^12.0",
        "illuminate/log": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "PakaiLink\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PakaiLink\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PakaiLink\\PakaiLinkServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
